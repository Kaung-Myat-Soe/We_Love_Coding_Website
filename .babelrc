{
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread"
  ],
  "presets": [
    "react",
    [
      "env",
      {
        "debug": true,
        "modules": false,
        "targets": {
          "browsers": [
            "chrome >= 56",
            "edge >= 14",
            "firefox >= 52",
            "ie >= 11",
            "ios >= 10",
            "opera >= 43",
            "safari >= 10.1"
          ]
        },
        "useBuiltIns": true
      }
    ]
  ]
}
